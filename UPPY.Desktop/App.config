<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <system.serviceModel>
    <client>
      <endpoint name="FileRepositoryService" address="net.tcp://192.168.1.220:5000" binding="netTcpBinding" contract="FileServer.Services.IFileRepositoryService" bindingConfiguration="customTcpBinding"/>
    </client>

    <bindings>
      <netTcpBinding>
        <binding name="customTcpBinding" maxReceivedMessageSize="20480000" transferMode="Streamed"/>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
</configuration>